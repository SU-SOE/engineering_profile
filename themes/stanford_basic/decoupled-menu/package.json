    "brotli-size": "^4.0.0",
    "css-loader": "^6.7.1",
    "dotenv": "^16.3.1",
    "glob": "^10.3.10",
    "gzip-size": "^6.0.0",
    "html-webpack-plugin": "^5.6.0",
    "kleur": "^4.1.5",
    "prettier": "^2.6.2",
    "pretty-bytes": "^5.0.0",
    "style-loader": "^3.3.1",
    "ts-node": "~10.8.0",
    "typescript": "^5.2.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.9.3"
  }
